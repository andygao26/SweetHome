                                @* 返回未讀有幾封信 以及未讀的信 ，並且可以點擊*@
                                <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
                                <script>
                                    $(document).ready(function () {
                                        $.get('/Email/UnreadEmailCount', function (data) {
                                            $('.badge-notification').text(data.count);
                                        });
                                    });

                                    $.get('/Email/UnreadEmails', function (data) {
                                        var menu = $('#notifications-dropdown .dropdown-menu');
                                        menu.empty();

                                        // 确保 data 包含未读邮件的正确属性
                                        data.forEach(function (email) {
                                            var emailLink = $('<a class="dropdown-item" href="/Email/CheckEmailContent/' + email.id + '">' + email.subject + '</a>');

                                            // 添加点击事件处理程序
                                            emailLink.click(function (e) {
                                                e.preventDefault(); // 阻止默认行为
                                                window.location.href = emailLink.attr('href'); // 跳转到邮件内容页面
                                            });

                                            var listItem = $('<li></li>');
                                            listItem.append(emailLink);
                                            menu.append(listItem);
                                        });
                                    });

                                </script>
                                @* \\返回未讀有幾封信  以及未讀的信，並且可以點擊*@
                               

